{
"version": "0.0.0",
"engines": {
  "node": "7.5.0"
},
"description": "A fullstack, multi-feature Angular2/Redux app made from several single-feature apps",
"license": "MIT",
"angular-cli": {},
"scripts": {
  "ng": "ng",
  "test": "ng test",
  "pree2e": "webdriver-manager update --standalone false --gecko false",
  "e2e": "protractor",
  "xbackend": "json-server --watch server/db/db.json --port 3001",
  "prebuild": "npm run clean",
  "clean": "rimraf dist/",
  "cover": "istanbul report --include=**/chrome/coverage-final.json text text-summary",
  "dev": "concurrently -r \"npm run dev:client\" \"npm run dev:server\"",
  "dev:client": "cross-env NODE_ENV=development ng serve --proxy-config proxy.conf.json",
  "dev:server": "cross-env NODE_ENV=development PORT=3100 nodemon server/node-server.js",
  "prod": "concurrently -r \"npm run prod:client\" \"npm run prod:server\"",
  "prod:client": "ng serve --proxy-config proxy.conf.json",
  "prod:server": "cross-env NODE_ENV=production PORT=3100 node server/node-server.js",
  "prod:server:heroku": "cross-env NODE_ENV=production node server/node-server.js",
  "start": "cross-env NODE_ENV=development ng serve --proxy-config proxy.conf.json",
  "start-both": "concurrently -r \"npm run prod:client\" \"npm run prod:server:heroku\"",
  "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
  "lint-css": "stylelint './src/**/*.css'",
  "lint-ts": "tslint './src/**/*.ts'",
  "lint-js": "eslint '**/*.js' --ignore-path .gitignore",
  "pretest": "rimraf coverage/ & npm run lint",
  "posttest": "npm run cover",
  "test:watch": "karma start --singleRun false",
  "debug-server": "cross-env NODE_ENV=development PORT=3100 nodemon --nolazy --debug-brk=5858 server/node-server.js",
  "postinstall": "ng build --prod"
},
"private": true,
