{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "great-big-angular2-example"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "../node_modules/kenya-project-pkg/src/index.html",
      "main": "../node_modules/kenya-project-pkg/src/main.ts",
      "polyfills": "../node_modules/kenya-project-pkg/src/polyfills.ts",
      "test": "../node_modules/kenya-project-pkg/src/test.ts",
      "tsconfig": "../node_modules/kenya-project-pkg/src/tsconfig.app.json",
      "testTsconfig": "../node_modules/kenya-project-pkg/src/tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/kenya-project-pkg/src/assets/styles/index.scss"
      ],
      "scripts": [],
      "environmentSource": "../node_modules/kenya-project-pkg/src/environments/environment.ts",
      "environments": {
        "dev": "../node_modules/kenya-project-pkg/src/environments/environment.ts",
        "prod": "../node_modules/kenya-project-pkg/src/environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
